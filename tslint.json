{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
        true,
        "single",
        "jsx-double"
    ],
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "ordered-imports": false,
    "no-console": false
  },
  "rulesDirectory": []
}
